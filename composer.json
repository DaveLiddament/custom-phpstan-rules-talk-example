{
    "name": "dave-liddament/custom-phpstan-rules-talk-demo",
    "description": "Demo of adding custom PHPStan rules to a project",
    "type": "project",
    "require": {
        "dave-liddament/custom-phpstan-rules-talk-demo-library": "^1.1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5",
        "dave-liddament/phpstan-rule-test-helper": "^0.5.0",
        "dave-liddament/custom-phpstan-rules-talk-upgrade-rule": "^1.0.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DaveLiddament\\CustomPhpstanRulesTalkDemo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DaveLiddament\\CustomPhpstanRulesTalkDemo\\Build\\": "build/"
        }
    },
    "authors": [
        {
            "name": "Dave Liddament",
            "email": "dave@lampbristol.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DaveLiddament/custom-phpstan-rules-talk-demo-upgrade-rule"
        },
        {
            "type": "vcs",
            "url": "https://github.com/DaveLiddament/custom-phpstan-rules-talk-demo-library"
        }
    ]
}
